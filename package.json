{
  "name": "@darkobits/log",
  "description": "The logger that darkobits uses.",
  "version": "1.1.3",
  "author": "darkobits",
  "license": "WTFPL",
  "keywords": [
    "log",
    "npm",
    "unique"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darkobits/log.git"
  },
  "files": [
    "dist"
  ],
  "main": "dist/log.js",
  "scripts": {
    "clean": "rimraf dist",
    "check:styles": "tslint --project tsconfig.json --format codeFrame",
    "check:types": "tsc --emitDeclarationOnly && rimraf **/*.spec.d.ts",
    "prebuild": "run-p clean check:*",
    "build": "babel src --extensions='.ts' --ignore='**/*.spec.ts,**/*.d.ts' --out-dir=dist --source-maps=inline",
    "prebump": "npm run build",
    "bump": "standard-version",
    "bump:beta": "npm run prebump && standard-version --prerelease=beta"
  },
  "dependencies": {
    "@darkobits/import-unique": "^1.0.0",
    "npmlog": "^4.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.51",
    "@babel/core": "^7.0.0-beta.51",
    "@babel/node": "^7.0.0-beta.51",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.51",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.51",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/preset-stage-3": "^7.0.0-beta.51",
    "@babel/preset-typescript": "^7.0.0-beta.51",
    "@types/node": "^10.3.4",
    "@types/npmlog": "^4.1.0",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "standard-version": "^4.4.0",
    "tslint": "^5.10.0",
    "tslint-xo": "^0.8.0",
    "typescript": "^2.9.1"
  }
}
